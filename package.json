{
  "name": "aws-application-accelerator",
  "version": "1.5.1",
  "description": "A model-driven AWS application framework using CDK v2",
  "main": "lib/app.js",
  "scripts": {
    "build": "tsc && npm run build:lambda && npm run build:layers",
    "build:lambda": "cd lib/lambda && npx tsc",
    "build:layers": "npm run build:layers:aws-sdk && npm run build:layers:cognito",
    "build:layers:aws-sdk": "cd layers/aws-sdk/nodejs && npm install --production",
    "build:layers:cognito": "cd layers/cognito/nodejs && npm install --production",
    "watch": "tsc -w",
    "test": "jest",
    "test:unit": "jest --testPathPattern=test/.*\\.test\\.ts$ --testPathIgnorePatterns=integration|e2e|acceptance",
    "test:integration": "jest --testPathPattern=test/integration/.*\\.test\\.ts$",
    "test:e2e": "jest --testPathPattern=test/e2e/.*\\.test\\.ts$",
    "test:acceptance": "jest --testPathPattern=test/acceptance/.*\\.test\\.ts$",
    "test:setting": "jest --testPathPattern=test/.*/setting.*\\.test\\.ts$",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "cdk": "cdk",
    "deploy": "npm run build && cdk deploy",
    "deploy:with-frontend": "npm run build && node scripts/deploy-with-frontend.js",
    "deploy:with-frontend:bash": "./scripts/deploy-with-frontend.sh",
    "deploy:with-frontend:bat": "scripts\\deploy-with-frontend.bat",
    "deploy:with-frontend:ps1": "powershell -ExecutionPolicy Bypass -File scripts/deploy-with-frontend.ps1",
    "destroy": "cdk destroy",
    "synth": "cdk synth",
    "diff": "cdk diff",
    "bootstrap": "cdk bootstrap",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "frontend:vue:dev": "cd frontend/vue && npm run dev",
    "frontend:vue:build": "cd frontend/vue && npm run build",
    "frontend:vue:install": "cd frontend/vue && npm install",
    "frontend:react:dev": "cd frontend/react && npm run dev",
    "frontend:react:build": "cd frontend/react && npm run build",
    "frontend:react:install": "cd frontend/react && npm install",
    "frontend:angular:dev": "cd frontend/angular && npm start",
    "frontend:angular:build": "cd frontend/angular && npm run build",
    "frontend:angular:install": "cd frontend/angular && npm install",
    "frontend:install:all": "npm run frontend:vue:install && npm run frontend:react:install && npm run frontend:angular:install",
    "migrate:component-name": "node scripts/migrate-component-name.js",
    "verify:component-migration": "node scripts/migrate-component-name.js --dry-run"
  },
  "devDependencies": {
    "@aws-amplify/api-graphql": "^3.4.22",
    "@aws-sdk/client-appsync": "^3.864.0",
    "@aws-sdk/client-cognito-identity-provider": "^3.848.0",
    "@aws-sdk/client-dynamodb": "^3.848.0",
    "@aws-sdk/client-secrets-manager": "^3.848.0",
    "@aws-sdk/util-dynamodb": "^3.864.0",
    "@types/aws-lambda": "^8.10.130",
    "@types/jest": "^29.5.5",
    "@types/node": "20.6.2",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "aws-amplify": "^5.3.27",
    "eslint": "^8.57.1",
    "graphql-request": "^6.1.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "~5.2.2"
  },
  "dependencies": {
    "@aws-cdk/aws-appsync-alpha": "2.59.0-alpha.0",
    "@aws-sdk/client-cognito-identity-provider": "^3.848.0",
    "@aws-sdk/client-dynamodb": "^3.848.0",
    "@aws-sdk/client-rds-data": "^3.848.0",
    "@aws-sdk/client-s3": "^3.850.0",
    "@aws-sdk/client-secrets-manager": "^3.848.0",
    "@aws-sdk/client-sqs": "^3.848.0",
    "@aws-sdk/lib-dynamodb": "^3.850.0",
    "@types/uuid": "^9.0.4",
    "aws-cdk-lib": "2.100.0",
    "commander": "^11.1.0",
    "constructs": "^10.0.0",
    "db-migrate": "^0.11.14",
    "db-migrate-mysql": "^2.2.0",
    "jsonwebtoken": "^9.0.2",
    "uuid": "^9.0.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/aws-application-accelerator.git"
  },
  "keywords": [
    "aws",
    "cdk",
    "appsync",
    "graphql",
    "serverless",
    "framework"
  ],
  "author": "Your Organization",
  "license": "MIT"
}
